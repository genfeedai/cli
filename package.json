{
  "name": "@genfeedai/cli",
  "version": "0.1.0",
  "description": "CLI tool for Genfeed.ai - Generate AI images and videos from your terminal",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "genfeed": "dist/index.js"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "build:standalone": "bun build src/index.ts --compile --outfile genfeed",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "keywords": [
    "genfeed",
    "ai",
    "image-generation",
    "video-generation",
    "cli"
  ],
  "author": "Genfeed.ai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genfeedai/cli.git"
  },
  "homepage": "https://genfeed.ai",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.1",
    "chalk": "^5.4.1",
    "commander": "^13.0.0",
    "conf": "^13.1.0",
    "ofetch": "^1.4.1",
    "ora": "^8.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "^1.1.14",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^2.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
